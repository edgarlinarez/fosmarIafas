<input type="hidden" id="hfCodProcedimiento" value="@ViewBag.id" />
<div class="container">
    <div class="row justify-content-center">
        <div class="col-11 px-0">
            <form id="FormularioProcedimiento" autocomplete="off">
                <div class="container">
                    <div class="form-row">
                        <h5 class="modal-title" id="exampleModalLabel">Agregar Procedimiento</h5>
                        <div class="col form-group px-1">
                            <div class="col text-right pt-3 px-1">
                                <button id="btnGrabarProcedimiento" type="button" class="btn btn-primary btn-xs">Grabar</button>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="container">
                    <div class="form-row">
                        <div class="col-2 form-group px-1">
                            <span class="required-form-span">Código</span>
                            <input type="hidden" id="hfCodigoProcedimiento" />
                            <input id="txtCodigo" name="txtCodigo" class="form-control form-control-sm text-uppercase text-right" type="text" maxlength="50" disabled readonly="readonly">
                        </div>
                        <div class="col form-group px-1">
                            <span class="required-form-span"> Descripción </span>
                            <input id="txtDescripcion" name="txtDescripcion" class="form-control form-control-sm text-uppercase alfanumerico" type="text" maxlength="50">
                        </div>
                        <div class="col form-group px-1">
                            <span class="required-form-span"> Abreviatura </span>
                            <input id="txtAbreviatura" name="txtAbreviatura" class="form-control form-control-sm text-uppercase alfanumerico" type="text" maxlength="50">
                        </div>
                    </div>
                </div>
                <div class="container">
                    <div class="form-row">
                        <div class="form-group col-md-2">
                            <span class="required-form-span"> TIP </span>
                            <input id="txtTip" name="txtTip" class="form-control form-control-sm text-uppercase alfanumerico" type="text" maxlength="50">
                        </div>
                        <div class="form-group col-md-2">
                            <span class="required-form-span">Sexo</span>
                            <select id="cboSexo" name="cboSexo" class="form-control form-control-sm"></select>
                        </div>
                        <div class="form-group col-md-4">
                            <span class="required-form-span">Grupo Procedimiento</span>
                            <div class="input-group input-group-sm">
                                <input type="hidden" id="hfIdGProcedimiento">
                                <input type="text" id="txtGProcedimiento" name="txtGProcedimiento" class="form-control disabled" readonly="readonly">
                                <div class="input-group-append">
                                    <button class="btn btn-secondary px-2" type="button" id="btnmdlGProcedimiento" data-toggle="modal" data-target="#mdlBuscarGProcedimiento">
                                        <i class="fa fa-search"></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                        <div class="col form-group col-md-2">
                            <span class="required-form-span">Medico Tratante</span>
                            <select id="cboMedicTratante" name="cboMedicTratante" class="form-control form-control-sm"></select>
                        </div>
                        <div class="col form-group col-md-2">
                            <span class="required-form-span">Piezas Dentales</span>
                            <select id="cboPiezasDentales" name="cboPiezasDentales" class="form-control form-control-sm"></select>
                        </div>
                    </div>
                </div>
                <div class="container">
                    <div class="form-row">
                        
                        @*PRUEBA BENEFICIO*@
                        <div class="form-group col-md-4 d-none">
                            <span class="required-form-span">Beneficio</span>
                            <div class="form-row">
                                <div class="input-group input-group-sm">
                                    <input type="hidden" id="hfIdBeneficio">
                                    <input type="text" id="txtBeneficio" class="form-control form-control-sm disabled" readonly="readonly">
                                    <div class="input-group-append">
                                        <button class="btn btn-secondary px-2" type="button" id="btnmdlBeneficio" data-toggle="modal" data-target="#mdlBuscarBeneficio">
                                            <i class="fa fa-search "></i>
                                        </button>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
                <div class="container">
                    <div class="form-row">
                        <div class="col-2 form-group px-1">
                            <span> Edad</span>
                            <div class="input-group input-group-sm">
                                <input id="txtEdadLimite" name="txtEdadLimite" class="form-control form-control-sm text-right pr-1" type="number" maxlength="3" placeholder="0" min="0" max="110">
                                <div class="input-group-append">
                                    <span class="input-group-text" id="basic-addon2">Lim. Cobertura</span>
                                </div>
                            </div>
                        </div>
                        <div class="col-2 form-group px-1">
                            <span class="required-form-span">Intervención Quirúrgica</span>
                            <select id="cboInterQuirur" name="cboInterQuirur" class="form-control form-control-sm"></select>
                        </div>
                        <div class="col-2 form-group px-1">
                            <span>Procedimiento Repetible</span>
                            <div class="input-group input-group-sm">
                                <input id="txtProcedRep" name="txtProcedRep" class="form-control form-control-sm text-right pr-1" type="number" maxlength="3" placeholder="0" min="0" max="100">
                                <div class="input-group-append">
                                    <span class="input-group-text" id="basic-addon2">Nro. Veces</span>
                                </div>
                            </div>
                        </div>
                        <div class="col-6">
                            <div class="form-row">
                                <div class="col-6 form-group px-1">
                                    <span class="required-form-span">Equiv. SUSALUD 1</span>
                                    <select id="cboEqSusalud1" name="cboEqSusalud1" class="form-control form-control-sm"></select>
                                </div>
                                <div class="col-6 form-group px-1">
                                    <span class="required-form-span">Equiv. SUSALUD 2</span>
                                    <select id="cboEqSusalud2" name="cboEqSusalud2" class="form-control form-control-sm"></select>
                                </div>
                                <div class="col form-group px-1 d-none">
                                    <span>Catálogo Tarifario</span>
                                    <select id="cboTarifario" name="cboTarifario" class="form-control form-control-sm"></select>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="container">
                    <div class="form-row">
                        <div class="col-2 form-group px-1">
                            <span>Cod. SEGUS</span>
                            <input type="text" id="txtCSegus" name="txtCSegus" class="form-control form-control-sm text-uppercase alfaNumCharEsp" required maxlength="150">
                        </div>
                        <div class="col-2 form-group px-1">
                            <span>Unidad SEGUS</span>
                            <input type="number" id="txtUSegus" name="txtUSegus" class="form-control form-control-sm text-right pr-1 numeroDecimal" step="0.01" min="0" max="999999" step="0.01" required maxlength="7" placeholder="0.00">
                        </div>

                        <div class="col-2 form-group px-1">
                            <span class="required-form-span"> Nomenclador</span>
                            <div class="input-group input-group-sm">
                                <input type="text" id="txtCNomenclador" name="txtCNomenclador" class="form-control form-control-sm disabled" readonly="readonly" />
                                <div class="input-group-append">
                                    <button class="btn btn-secondary px-2" type="button" id="btnmdlnomeclador" data-toggle="modal" data-target="#mdlBuscarnomeclador">
                                        <i class="fa fa-search "></i>
                                    </button>
                                </div>
                            </div>
                        </div>
                        @*<div class="col-2 form-group px-1">
            <span>Cod. Nomenclador</span>
            <input type="text" id="txtCNomenclador" name="txtCNomenclador" class="form-control form-control-sm text-uppercase alfaNumCharEsp" required maxlength="150">
        </div>*@
                        <div class="col-2 form-group px-1">
                            <span>Unidad Nomenclador</span>
                            <input type="number" id="txtUNomenclador" name="txtUNomenclador" class="form-control form-control-sm text-right pr-1 numeroDecimal" min="0" max="999999" step="0.01" required maxlength="7" placeholder="0.00">
                        </div>
                    </div>
                </div>
                <div class="container">
                    <div class="form-row">
                        <div class="col-3 d-none">
                            <span>Unidad</span>
                            <input type="text" id="txtUnidad" name="txtUnidad" class="form-control form-control-sm text-uppercase" step="0.01" required maxlength="150" placeholder="0.00">
                        </div>
                    </div>
                </div>
                <partial name="/Views/Shared/Partials/_AuditoriaPartial2.cshtml" />
            </form>
            <div class="modal fade" id="mdlBuscarnomeclador" data-backdrop="static" data-keyboard="false" tabindex="-1" aria-labelledby="mdlBuscarnomeclador" aria-hidden="true">
                <div class="modal-dialog modal-lg">
                    <div class="modal-content">
                        <div class="modal-header">
                            <h5 class="modal-title" id="mdlBuscarServcio">Busqueda Nomenclador</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                                <span aria-hidden="true">&times;</span>
                            </button>
                        </div>
                        <div class="modal-body">
                            <div class="row">
                                <div class="col">
                                    <table id="dtnomenclador" class="table table-striped table-bordered"></table>
                                </div>
                            </div>
                        </div>
                        <div class="modal-footer">
                            <button type="button" class="btn btn-secondary btn-xs" data-dismiss="modal">Cerrar</button>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
</div>
