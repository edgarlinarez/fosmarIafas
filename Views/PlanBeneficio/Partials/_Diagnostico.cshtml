<div class="">
    <div class="form-row">
        <button type="button" id="btnNuDiagnostico" class="btn btn-primary btn-xs btn-nuevo" data-toggle="modal" data-target="#mdlDiagnostico">
            <i class="fa fa-plus font-weight-bold ml-1"></i> Agregar
        </button>
    </div>
    <div class="form-row">
        <table id="dtDiagnosticoExcluido" class="table table-striped table-bordered"></table>
    </div>
</div>
<div class="modal fade" id="mdlDiagnostico" data-backdrop="static" data-keyboard="false" tabindex="-1">
    <div class="modal-dialog modal-lg">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title" id="titleDiagnostico">Nuevo Diagnóstico</h5>
                <button type="button" class="close" data-dismiss="modal">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <form id="frmDiagnostico" autocomplete="off">
                    <div class="container">
                        <div class="form-row form-group d-none">
                            <div class="col-lg-6">
                                <span>Código</span>
                               
                                <input type="text" id="txtCodigoDiag" class="form-control form-control-sm disabled" readonly="readonly" />
                            </div>
                            <div class="col-lg-6">
                                <span>Descripción</span>
                                <input type="text" id="txtDescripcionDiag" name="txtDescripcionDiag" class="form-control form-control-sm" />
                            </div>
                        </div>
                        <div class="form-row">
                            <div class="col-lg-12">
                                <table id="dtDiagnostico" class="table table-striped table-bordered"></table>
                            </div>
                        </div>
                    </div>
                </form>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-secondary btn-xs" data-dismiss="modal">Cerrar</button>
                <button id="btnGrDiagnostico" type="button" class="btn btn-primary btn-xs">Guardar</button>
            </div>
        </div>
    </div>
</div>
<input type="hidden" id="hfIdDiagnosticoExcl" />
