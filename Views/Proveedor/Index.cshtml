
@{
    ViewData["Title"] = "Proveedor";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<ul class="nav nav-tabs" id="tabProveedor" role="tablist">
    <li class="nav-item">
        <a class="nav-link active" id="Lista-tab" data-toggle="tab" href="#listar" role="tab" aria-controls="listar" aria-selected="true">Listado</a>
    </li>
    <li class="nav-item">
        <a class="nav-link " id="Datos-tab" data-toggle="tab" href="#datos" role="tab" aria-controls="datos" aria-selected="false">Datos Proveedor</a>
    </li>
    <li class="nav-item">
        <a class="nav-link " id="Equivalencia-tab" data-toggle="tab" href="#equivalencia" role="tab" aria-controls="equivalencia" aria-selected="false">Equivalencia</a>
    </li>
    <li class="nav-item">
        <a class="nav-link " id="Sucursal-tab" data-toggle="tab" href="#sucursal" role="tab" aria-controls="sucursal" aria-selected="false">Sucursal</a>
    </li>
</ul>
<div class="tab-content" id="myTabContent">
    <div class="tab-pane fade show active pt-3 border-right border-bottom border-left" id="listar" role="tabpanel" aria-labelledby="Lista-tab">
        <partial name="Partials/_ListaProveedor" />
    </div>
    <div class="tab-pane fade pt-3 border-right border-bottom border-left" id="datos" role="tabpanel" aria-labelledby="Datos-tab">
        <partial name="Partials/_AgregarProveedor" />
    </div>
    <div class="tab-pane fade pt-3 border-right border-bottom border-left" id="equivalencia" role="tabpanel" aria-labelledby="Equivalencia-tab">
        <partial name="Partials/_Equivalencia" />
    </div>
    <div class="tab-pane fade pt-3 border-right border-bottom border-left" id="sucursal" role="tabpanel" aria-labelledby="Sucursal-tab">
        <partial name="Partials/_Sucursal" />
    </div>
</div>
<input type="hidden" id="hfactionProveedor" />

@section Scripts{
    <script asp-append-version="true" src="~/js/views/Proveedor/index.js"></script>
    <script asp-append-version="true" src="~/js/ubigeos.js"></script>
}